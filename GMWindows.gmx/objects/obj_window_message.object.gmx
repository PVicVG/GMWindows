            <string>/// "Ok"/ Close Window
if global.win_in_focus == id
{
    if (mouse_x &gt; (x + ((window_width * window_scale) / 2)) - 12) &amp;&amp; (mouse_x &lt;  (x + ((window_width * window_scale) / 2)) + 12) &amp;&amp; (mouse_y &gt; (y + (window_height * window_scale) - 8) - 12) &amp;&amp; (mouse_y &lt; (y + (window_height * window_scale) - 8 + 12))
    {
        can_ok = true;
    }
    else
    {
        can_ok = false;
